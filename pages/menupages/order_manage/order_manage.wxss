.order-info-view{
  border-radius: 16rpx;
}
.order-info-item{
  padding: 10rpx 0rpx;
}
.order-add-plate-input{
  display: inline-block;
  width: 240rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin:10rpx;
  background-color: white;
  border: 2rpx solid #dddee1;
  border-radius: 12rpx;
  text-align: left;
}
.order-add-time-input{
  display: inline-block;
  width: 60rpx;
