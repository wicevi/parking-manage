  display: inline-block;
  width: 100rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin: 10rpx 5rpx;
  background-color: white;
  border: 2rpx solid #dddee1;
  border-radius: 12rpx;
  text-align: center;
}
